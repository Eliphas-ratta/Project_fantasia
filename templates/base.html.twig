<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>âš«</text></svg>">
{{ encore_entry_link_tags('app') }}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {% block importmap %}{{ importmap('app') }}{% endblock %}
    {% endblock %}
    
    
</head>

<header class=" text-white py-3">
    <div class="d-flex justify-content-between align-items-center">
        <div>
        <h1 class="me-3">Project Fantasia</h1>
        </div>
        <nav class="navhead">
            <ul class="d-flex gap-3 list-unstyled mb-0">
                <li><a href="{{ path('app_home') }}" class="text-white text-decoration-none">Home</a></li>
                <li><a href="{{ path('app_world') }}" class="text-white text-decoration-none">World</a></li>

            </ul>
        </nav>
        <nav>
            <ul class="d-flex gap-3 list-unstyled mb-0">
                
                <li><a href="{{ path('app_profile') }}" class="text-white text-decoration-none">Profile</a></li>
                <li><a href="{{ path('app_logout') }}" class="text-danger text-decoration-none">Logout</a></li>
            </ul>
        </nav>

        
    </div>
</header>

<body>
    {% block body %}{% endblock %}
    {{ encore_entry_script_tags('app') }}
    
</body>
</html>
